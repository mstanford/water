library test
library filesystem
library string



test sln2005
	var path "samples/Sln2005/ACLChange.sln.txt"

	var solution ((method (typeof Bamboo.VisualStudio.VisualStudio2005.Solution.SolutionReader Bamboo.VisualStudio) Read) path)
	var s (file.to-string path)
	var stream (memory-stream)
	var encoding ((property (typeof System.Text.Encoding) UTF8))
	var writer (new (typeof System.IO.StreamWriter) stream encoding)
	((method (typeof Bamboo.VisualStudio.VisualStudio2005.Solution.SolutionWriter Bamboo.VisualStudio) Write) solution writer)
	= stream.Position 0
	assert.equals s (stream.to-string stream)
end_test

